{
  "name": "damianapp",
  "version": "1.0.0",
  "description": "Una aplicación React Native desarrollada con Expo que demuestra las mejores prácticas de desarrollo",
  "main": "index.js",
  "author": "Damian",
  "license": "MIT",
  "keywords": [
    "react-native",
    "expo",
    "mobile-app",
    "eslint",
    "prettier"
  ],
  "scripts": {
    "_comment_development": "Scripts para desarrollo y ejecución",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "_comment_quality": "Scripts para calidad y formateo de código",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint && npm run format:check",
    "_comment_testing": "Scripts para testing y verificación",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:config": "jest --testPathPattern=config",
    "test:services": "jest --testPathPattern=services",
    "test:components": "jest --testPathPattern=components",
    "verify": "node VERIFICACION_COMPLETA.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/drawer": "^7.5.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "expo": "53.0.16",
    "expo-av": "~15.1.7",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-haptics": "^14.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-status-bar": "~2.2.3",
    "prettier": "^3.6.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.9.0",
    "eslint": "^9.29.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.7.0",
    "react-test-renderer": "19.0.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.expo/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.js",
      "!src/**/*.test.{js,jsx}",
      "!src/**/*.spec.{js,jsx}",
      "!src/utils/testUtils.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@expo|expo-.*)/)"
    ]
  },
  "private": true
}
